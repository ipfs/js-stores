{
  "name": "interface-datastore",
  "version": "7.0.0",
  "description": "datastore interface",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/js-ipfs-interfaces/tree/master/packages/interface-datastore#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs-interfaces.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/js-ipfs-interfaces/issues"
  },
  "keywords": [
    "datastore",
    "interface",
    "ipfs",
    "key-value"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist/src",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./key": {
      "types": "./dist/src/key.d.ts",
      "import": "./dist/src/key.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "scripts": {
    "build": "aegir build",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "clean": "aegir clean",
    "test": "aegir test",
    "test:node": "aegir test -t node --cov",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "release": "aegir release"
  },
  "dependencies": {
    "interface-store": "^3.0.0",
    "nanoid": "^4.0.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "aegir": "^37.5.0"
  }
}
