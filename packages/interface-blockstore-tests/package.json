{
  "name": "interface-blockstore-tests",
  "version": "2.0.5",
  "description": "Compliance tests for the blockstore interface",
  "leadMaintainer": "Alex Potsides <alex.potsides@protocol.ai>",
  "main": "src/index.js",
  "type": "module",
  "types": "types/src/index.d.ts",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "aegir build",
    "lint": "aegir ts -p check && aegir lint",
    "dep-check": "aegir dep-check -i rimraf",
    "clean": "rimraf dist types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/interface-blockstore.git"
  },
  "keywords": [
    "interface",
    "key-value",
    "ipfs",
    "blockstore"
  ],
  "license": "(Apache-2.0 OR MIT)",
  "bugs": {
    "url": "https://github.com/ipfs/interface-blockstore/issues"
  },
  "homepage": "https://github.com/ipfs/js-ipfs-interfaces/tree/master/packages/interface-blockstore-tests#readme",
  "dependencies": {
    "aegir": "^37.4.8",
    "interface-blockstore": "^2.0.3",
    "it-all": "^1.0.2",
    "it-drain": "^1.0.1",
    "it-length": "^1.0.2",
    "multiformats": "^9.1.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "publishConfig": {
    "directory": "dist"
  }
}
